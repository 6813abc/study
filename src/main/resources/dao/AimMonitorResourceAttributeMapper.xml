<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyg.study.dao.AimMonitorResourceAttributeDao">
    <resultMap id="AimMonitorResourceAttributeMap" type="com.cyg.study.bean.AimMonitorResourceAttribute">
        <id column="id" jdbcType="BIGINT" property="id" />
        <result column="resource_id" jdbcType="VARCHAR" property="resourceId" />
        <result column="key" jdbcType="VARCHAR" property="key" />
        <result column="value" jdbcType="VARCHAR" property="value" />
    </resultMap>


    <select id="selectOneAttrByResourceIdAndKey" parameterType="com.cyg.study.bean.AimMonitorResourceAttribute"
            resultType="com.cyg.study.bean.AimMonitorResourceAttribute">
        select * from aim_monitor_resource_attribute
        where resource_id=#{resourceId}
        and `key` = #{key}
        limit 1
    </select>
</mapper>